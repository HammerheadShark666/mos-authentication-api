	},
	"item": [
		{
			"name": "Authorization",
			"item": [
				{
					"name": "{{authorization_url}}/api/{version}login",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "0dc42615-78d0-4ede-bb9c-440612134b92",
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});  \r",
									"\r",
									"pm.test('Schema validation', () => {\r",
									"    pm.response.to.have.jsonSchema(JSON.parse(pm.globals.get(\"logged_in_schema\")));\r",
									"});\r",
									" \r",
									"var jsonData = JSON.parse(pm.response.text()); \r",
									"pm.environment.set(\"token\", jsonData.jwtToken);"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"id": "9edb760c-d117-47a4-a3a7-36d50803ad38",
					"protocolProfileBehavior": {
						"disableBodyPruning": true
					},
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"username\": \"{{username}}\",\r\n  \"password\": \"{{password}}\"\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": "https://{{authorization_url}}/api/v1/login"
					},
					"response": []
				},
